{
  "name": "ill.cloud",
  "version": "0.2.1",
  "private": true,
  "resolve": {
    "fallback": {
      "crypto": false
    }
  },
  "scripts": {
    "dev": "bun run --watch ./src/app.ts",
    "build": "bun build ./src/app.ts --target node --outdir ./dist",
    "start": "NODE_ENV=production bun ./src/app.ts",
    "db:docker": "docker compose -f docker-compose.db.yml up",
    "db:reset": "./src/utils/reset-db.sh",
    "check": "bunx @biomejs/biome check ./src",
    "check:fix": "bunx @biomejs/biome check --write ./src",
    "test": "bun test"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.0.2",
    "@elysiajs/cors": "^1.0.2",
    "@elysiajs/swagger": "^1.0.5",
    "@sentry/bun": "^9.3.0",
    "@supabase/supabase-js": "^2.47.11",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node-steam-openid": "^1.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.38.4",
    "elysia": "latest",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "node-steam-openid": "^2.0.0",
    "openid-client": "^6.1.7",
    "pg": "^8.11.5"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@types/pg": "^8.11.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "axios": "^1.6.8",
    "bun-types": "latest",
    "codecov": "^3.8.3",
    "drizzle-kit": "^0.30.2",
    "postgres": "^3.4.4",
    "supabase": "^2.6.8",
    "supertest": "^7.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.9.3",
    "uuid": "^9.0.1",
    "yoctocolors": "^2.1.1"
  },
  "module": "src/app.ts",
  "trustedDependencies": [
    "core-js",
    "core-js-pure",
    "es5-ext",
    "esbuild",
    "sharp"
  ]
}
