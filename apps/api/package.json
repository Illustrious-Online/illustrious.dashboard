{
  "name": "api",
  "version": "1.0.50",
  "scripts": {
    "dev": "NODE_ENV=development bun --watch src/index.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target node --outfile ./dist/index.js ./src/index.ts",
    "start": "bun start src/index.ts"
  },
  "dependencies": {
    "@elysiajs/eden": "^1.2.0",
    "elysia": "latest"
  },
  "devDependencies": {
    "@types/bun": "^1.1.14",
    "bun-types": "^0.5.0",
    "typescript": "^5.0.0"
  },
  "type": "module",
  "module": "src/index.js",
  "engines": {
    "bun": "latest"
  },
  "vercel": {
    "functions": {
      "app/elysia.ts": {
        "runtime": "bun"
      }
    }
  }
}