{
  "name": "api.illustrious.cloud",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development bun run --watch ./src/app.ts",
    "build": "./deploy-vercel.sh",
    "start": "NODE_ENV=production bun ./src/app.ts",
    "db:reset": "./src/utils/reset-db.sh",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:push": "bunx drizzle-kit push",
    "check": "biome check --write ./",
    "test": "set -o pipefail && ./src/utils/run-tests.sh"
  },
  "dependencies": {
    "@elysiajs/bearer": "^1.2.0",
    "@elysiajs/cors": "^1.2.0",
    "@elysiajs/swagger": "^1.2.0",
    "@supabase/supabase-js": "^2.47.10",
    "@types/jsonwebtoken": "^9.0.7",
    "dotenv": "^16.4.7",
    "drizzle-orm": "^0.38.3",
    "elysia": "latest",
    "generate-password-ts": "^1.6.5",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "pg": "^8.13.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.3.0",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/uuid": "^10.0.0",
    "axios": "^1.7.9",
    "bun-types": "latest",
    "drizzle-kit": "^0.30.1",
    "postgres": "^3.4.5",
    "semantic-release": "^24.2.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "uuid": "^11.0.3",
    "yoctocolors": "^2.1.1"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "module": "src/app.ts",
  "trustedDependencies": [
    "core-js",
    "core-js-pure",
    "es5-ext",
    "esbuild",
    "sharp"
  ]
}
