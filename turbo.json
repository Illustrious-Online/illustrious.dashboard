{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": true
    },
    "test": {
      "dependsOn": ["^build", "build"],
      "outputs": []
    },
    "check": {
      "dependsOn": ["^check"]
    },
    "check:fix": {
      "dependsOn": ["^check:fix"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "release": {
      "dependsOn": ["^release", "build"]
    }
  }
}
